   "source": [
    "import cv2\n",
    "\n",
    "cap = cv2.VideoCapture(0)\n",
    "\n",
    "while True:\n",
    "    ret, im = cap.read()\n",
    "    blur = cv2.GaussianBlur(im, (0, 0), 5)\n",
    "    cv2.imshow('camera capture', blur)\n",
    "    key = cv2.waitKey(10)\n",
    "    if key == 27:  # ESCキーで終了\n",
    "        break\n",
    "\n",
    "cap.release()\n",
    "cv2.destroyAllWindows()\n"
   ]
  },
